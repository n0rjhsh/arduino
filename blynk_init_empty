#define BLYNK_PRINT Serial
#define BLYNK_TEMPLATE_ID "_"
#define BLYNK_DEVICE_NAME "_"
#define BLYNK_AUTH_TOKEN "_"

#include <ESP8266WiFi.h>
#include <BlynkSimpleEsp8266.h>

char auth[] = "_";
char ssid[] = "_";
char pass[] = "_";

void setup()
{
  Serial.begin(9600);
  Blynk.begin(auth, ssid, pass);
}

void loop()
{
  Blynk.run();
}
