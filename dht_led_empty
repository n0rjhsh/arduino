#include <SimpleDHT.h>
#define _ _
#define _ _

SimpleDHT11 dht11(_);
byte temperature = 0;
byte humidity = 0;

void setup()
{
  Serial.begin(9600);
  pinMode(_, OUTPUT);
  pinMode(_, INPUT);
}

void loop()
{
  dht11.read(&temperature, &humidity, NULL);
  int _ = temperature;
  int _ = humidity;

  Serial._(_); 
  Serial._("_");
  Serial._(_); 
  Serial._("_");
  delay(_);

  if (_ > _)
  {
    _(_, HIGH);
  }
  else
  {
    digitalWrite(_, LOW);
  }
}#include <SimpleDHT.h>
